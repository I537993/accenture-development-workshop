{
  "name": "openui5-basic-template-app",
  "version": "0.1.0",
  "description": "Best-practice starting point for building a freestyle app",
  "private": true,
  "scripts": {
    "start": "ui5 serve -o /index.html",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && rimraf coverage && npm run karma-ci",
    "build": "rimraf dist && ui5 build --a",
    "postbuild": "cd dist && bestzip content.zip *"
  },
  "devDependencies": {
    "@ui5/cli": "^3",
    "bestzip": "^2.2.1",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.6.0",
    "rimraf": "^5",
    "ui5-middleware-livereload": "^0.7.0"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload"
    ]
  }
}
